{
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://0.0.0.0:8080"
      },
      "Https": {
        "Url": "https://0.0.0.0:8081"
      }
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore.Hosting.Diagnostics": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.AspNetCore.HttpLogging.HttpLoggingMiddleware": "Warning",
      "Microsoft.AspNetCore.Routing.EndpointMiddleware": "Warning"
    },
    "Filter": {
      "Microsoft.AspNetCore.Mvc.Infrastructure.ControllerActionInvoker": {
        "LogLevel": {
          "Microsoft.AspNetCore.Mvc.Infrastructure.ControllerActionInvoker.HealthController": "Warning"
        }
      }
    }
  }
} 